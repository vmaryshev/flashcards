<table>
	<tbody>
		<% @cards.each do |card| %>
			<tr>
				<td><%= card.id %></td>
				<td><%= card.original_text %></td>
				<td><%= card.translated_text %></td>
				<td><%= card.review_date %></td>
				<td>
					<%= link_to("Редактировать", edit_card_path(card))%>
					<%= link_to("Удалить", card_path(card), method: :delete)%>
					
				</td>	
			</tr>
		<% end %>
	</tbody>
</table>